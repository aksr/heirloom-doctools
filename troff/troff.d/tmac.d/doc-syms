.\" NS Ux macro - UNIX
.de Ux
.nr cF \\n(.f
.nr cZ \\n(.s
.ds aa \&\f\\n(cF\s\\n(cZ
.as b1 \&\\*(tNUNIX\\*(aa
.rm aa
.if \\n(aC==0 \{\
.       if \\n(.$>0 .aV \\$*
.\}
.ie \\n(aC>\\n(aP \{\
.       nr aP \\n(aP+1
.       ie \\n[C\\n(aP]==1 \{\
.               \\*[A\\n(aP]
.       \}
.       el .nR
.\}
.el .aZ
..
.\" NS Bx macro -BSD (fix smaller nroff version)
.de Bx
.nr cF \\n(.f
.nr cZ \\n(.s
.ds aa \&\f\\n(cF\s\\n(cZ
.if \\n(aC==0 \{\
.\" .       ie \\n(.$==0 \&\\*(tNBSD\\*(aa \\*(tNUNIX\\*(aa
.       ie \\n(.$==0 \&\\*(tNBSD\\*(aa
.       el .aV \\$*
.\}
.if \\$1-alpha \{\
\&currently in alpha test.
.	aY
.\}
.if \\$1-beta \{\
\&currently in beta test.
.	aY
.\}
.if \\$1-devel \{\
\&currently under development.
.	aY
.\}
.if \\n(aC>\\n(aP \{\
.       nr aP \\n(aP+1
.	ie \\n[C\\n(aP]==2 \{\
.		as b1 \&\\*[A\\n(aP]\&\\*(tNBSD\\*(aa
.		ie \\n(aC>\\n(aP \{\
.			nr jj \\n(aP+1
.			ie \\n[C\\n(jj]==2 \{\
.				ie \\*[A\\n(jj]Reno \{\
.					nr aP \\n(aP+1
.					as b1 \&\-\\*[A\\n(jj]
.				\}
.				el .ie \\*[A\\n(jj]reno \{\
.					nr aP \\n(aP+1
.					as b1 \&\-Reno
.				\}
.				el .ie \\*[A\\n(jj]Tahoe \{\
.					nr aP \\n(aP+1
.					as b1 \&\-\\*[A\\n(jj]
.				\}
.				el .ie \\*[A\\n(jj]tahoe \{\
.					nr aP \\n(aP+1
.					as b1 \&\-Tahoe
.				\}
.				el .if !\\*[A\\n(jj] .as b1 \&\-
.				ie \\n(aC>\\n(aP \{\
.					nr aP \\n(aP+1
.					nR
.				\}
.				el .aZ
.			\}
.			el \{\
.				nr aP \\n(aP+1
.				nR
.			\}
.			rr jj
.		\}
.		el .aZ
.	\}
.	el \{\
.	       as b1 \&\\*(tNBSD\\*(aa
.	       nR
.	\}
.\}
..
.\" Ns Ud macro - prints "currently under development" (HISTORY section)
.de Ud
\&currently under development.
..
.\" Ns At macro - AT&T UNIX
.de At
.	nr cF \\n(.f
.	nr cZ \\n(.s
.	ds aa \&\f\\n(cF\s\\n(cZ
.	ds A1 \\$1
.	aU 1
.	ie \\n(.$=0:(\\n(aT>2) \{\
.		ds b1 \&\\*(tNAT&T UNIX\\*(aa
.		if \\n(.$ .as b1 \\$1
.	\}
.	el \{\
.		ie \\$132v     .ds b1 \&Version 32V \\*(tNAT&T UNIX
.		el .ie \\$1v1  .ds b1 \&Version 1 \\*(tNAT&T UNIX
.		el .ie \\$1v2  .ds b1 \&Version 2 \\*(tNAT&T UNIX
.		el .ie \\$1v3  .ds b1 \&Version 3 \\*(tNAT&T UNIX
.		el .ie \\$1v4  .ds b1 \&Version 4 \\*(tNAT&T UNIX
.		el .ie \\$1v5  .ds b1 \&Version 5 \\*(tNAT&T UNIX
.		el .ie \\$1v6  .ds b1 \&Version 6 \\*(tNAT&T UNIX
.		el .ie \\$1v7  .ds b1 \&Version 7 \\*(tNAT&T UNIX
.		el .ie \\$1III .ds b1 \&\\*(tNAT&T\\*(aa System III \\*(tNUNIX
.		el .ie \\$1V   .ds b1 \&\\*(tNAT&T\\*(aa System V \\*(tNUNIX
.		el .ie \\$1V.1 .ds b1 \&\\*(tNAT&T\\*(aa System V Release 1 \\*(tNUNIX
.		el .ie \\$1V.2 .ds b1 \&\\*(tNAT&T\\*(aa System V Release 2 \\*(tNUNIX
.		el .ie \\$1V.3 .ds b1 \&\\*(tNAT&T\\*(aa System V Release 3 \\*(tNUNIX
.		el .ie \\$1V.4 .ds b1 \&\\*(tNAT&T\\*(aa System V Release 4 \\*(tNUNIX
.		el .tm .At: Unknown AT&T UNIX version "\\$1" (#\\n(.c)
.		as b1 \\*(aa
.		ie \\n(.$=2 .as b1 \\$2
.		el .if \\n(.$>2 .tm .At: More than 2 arguments (#\\n(.c)
.	\}
.	aZ
..
.de XBSDx
.	if !\\n(aC \{\
.		if \\n(.$ \{\
.			ds mN \\*[XBSDx_mN]
.			ds A1 \\$1
.			ds A2 \\$2
.			nr aC \\n(.$
.			nr aP 0
.		\}
.	\}
.	if \\n(aC>\\n(aP \{\
.		nr aP \\n(aP+1
.		aU \\n(aP
.		ie \\n(aT=1 .\\*[A\\n(aP]
.		el \{\
.			ie \\n(aT=2 .as b1 \ 
.			el .if !\\n(aT=3 .tm .\\*[XBSDx_mN]: \\*[A\\n(aP]: Wrong argument type (#\\n(.c)
.			as b1 \\*[A\\n(aP]
.			XBSDx
.		\}
.	\}
.	aZ
..
.de Ox
.	ds XBSDx_mN Ox
.	if !\\*(mN\\*[XBSDx_mN] .as b1 OpenBSD
.	XBSDx \\$1 \\$2
..
.de Nx
.	ds XBSDx_mN Nx
.	if !\\*(mN\\*[XBSDx_mN] .as b1 NetBSD
.	XBSDx \\$1 \\$2
..
.de Fx
.	ds XBSDx_mN Fx
.	if !\\*(mN\\*[XBSDx_mN] .as b1 FreeBSD
.	XBSDx \\$1 \\$2
..
.\" The Bt macro should go away now
.\" Ns Bt macro - prints "is currently in beta test." (HISTORY section)
.de Bt
\&is currently in beta test.
..
.\" NS St macro - standards (posix, ansi - formal standard names)
.ds Px \\*(tNPOSIX
.ds Ai \\*(tNANSI
.de St
.if \\n(aC==0 \{\
.	ie \\n(.$==0 \{\
.tm Usage: .St [-p1003.1-88 | -p1003.2 | -ansiC-89 | -iso ] \\*(Pu ... (#\\n(.c)
.	\}
.	el \{\
.		ds mN St
.		nr aP 0
.		nr fV \\n(.$
.		lnr _i_ 0 1
.		while \\n(.$ \{\
.			ds A\\n+[_i_] \\$1
.			shift
.		\}
.		fV
.	\}
.\}
.if \\n(aC>\\n(aP \{\
.	nr cF \\n(.f
.	nr cZ \\n(.s
.	ds aa \&\f\\n(cF\s\\n(cZ
.	nr aP \\n(aP+1
.	ie     \\*[A\\n(aP]-p1003.1 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1b \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1b\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1b\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1-88 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1-1988\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1-90 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1-1990\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1b-93 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1b-1993\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1b\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1-96 \{\
.		ds b1 \&\\*(tNISO/IEC\\*(aa 9945-1:1996\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1d-99 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1d-1999\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1d\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1g-2000 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1g-2000\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1g\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1-2001 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1-2001\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1-2004 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1-2004\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.1-2008 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.1-2008\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.1\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.2 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.2\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.2\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-p1003.2-92 \{\
.		ds b1 \&\\*(tNIEEE Std\\*(aa 1003.2-1992\\*(sV
.		as b1 (\\*(Lq\\*(tN\\*(Px.2\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-ansiC \{\
.		ds b1 \&\\*(tNANSI\\*(aa X3.159-1989\\*(sV
.		as b1 (\\*(Lq\\*(tNANSI C89\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-ansiC-89 \{\
.		ds b1 \&\\*(tNANSI\\*(aa X3.159-1989\\*(sV
.		as b1 (\\*(Lq\\*(tNANSI C89\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-ansiC-99 \{\
.		ds b1 \&\\*(tNANSI/ISO/IEC\\*(aa 9899-1999\\*(sV
.		as b1 (\\*(Lq\\*(tNANSI C99\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-isoC-99 \{\
.		ds b1 \&\\*(tNISO/IEC\\*(aa 9899:1999\\*(sV
.		as b1 (\\*(Lq\\*(tNISO C99\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-isoC-amd1 \{\
.		ds b1 \&\\*(tNISO/IEC\\*(aa 9899/AMD1:1995\\*(sV
.		as b1 (\\*(Lq\\*(tNISO C90, Amendment 1\\*(aa\\*(Rq)
.	\}
.	el .ie \\*[A\\n(aP]-iso8601 \{\
.		ds b1 \&\\*(tNISO\\*(aa 8601\\*(sV
